<?xml version="1.0" encoding="utf-8" ?>

<odoo>
    <record id="vitosec_website_portal_rma_form" model="ir.ui.view">
        <field name="name">Vitosec Website RMA Portal</field>
        <field name="model">rma</field>
        <field name="inherit_id" ref="rma.rma_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='partner_invoice_id']" position="after">
               <field name="order_id" />
            </xpath>
        </field>
    </record>
</odoo>
